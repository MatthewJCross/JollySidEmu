<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Resources\C64Sid.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="C64Sid.ico" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\C64Sid.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="NAudio" Version="2.2.1" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="C64Sid.ico" />
  </ItemGroup>

</Project>
